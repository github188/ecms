<!-- 消费报表 -->
<meta charset="UTF-8" >
<link href="order/order.css" type="text/css" rel="stylesheet">
<div class="panel panel-default">
  <div class="panel-body">
    <div class="form-horizontal">
      <div class="slide-content">
        <div class="form-group inblock inblock-lg">
          <label class="col-sm-2 control-label">关键字</label>
          <input class="form-control" name="keywords" type="text" placeholder="请输入订单编号、人员姓名查询" >
        </div>
        <div class="form-group inblock search-date">
          <label class="col-sm-2 control-label">订单开始时间</label>
          <input class="form-control laydate-icon" id="starttime" name="starttime" type="text" readonly >
        </div>
        <div class="form-group inblock">
          <label class="control-label col-sm-1" style="text-align: center;">至</label>
          <input class="form-control laydate-icon" id="endtime" name="endtime" type="text" readonly >
        </div>
        <div class="clearboth"></div>
        <div class="form-group inblock inblock-lg">
          <label class="col-sm-2 control-label">订单状态</label>
          <div class="select-form-kind">
            <select class="form-control" name="status">
              <option value="">--全部--</option>
            </select>
          </div>
        </div>
        <div class="form-group inblock search-date">
          <label class="col-sm-2 control-label">订单结束时间</label>
          <input class="form-control laydate-icon" id="stime" name="stime" type="text" readonly >
        </div>
        <div class="form-group inblock">
          <label class="control-label col-sm-1" style="text-align: center;">至</label>
          <input class="form-control laydate-icon" id="etime" name="etime" type="text" readonly >
        </div>

      </div>
      <div class="clearboth"></div>
      <div class="clearboth button-position">
        <button id="btnsearch" class="btn btn-orange btn-search">查询</button>
      </div>
    </div>
  </div>
</div>
<div class="panel panel-nopadding">
  <div class="panel-body">
    <div class="list-top">
      <h4 class="list-title">对账单</h4>
      <div class="button-power-position">
        <button class="btn btn-turquoise btn-export">导出</button>
      </div>
    </div>
    <div class="table-responsive" id="data_list">
      <table class="dataTable" cellspacing="0" width="100%">
        <thead>
        <tr>
          <th>订单编号</th>
          <th>洗车员</th>
          <th>订单金额</th>
          <th>订单状态</th>
          <th>订单开始时间</th>
          <th>订单结束时间</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          <td>002017020710002100</td>
          <td>张三</td>
          <td>38</td>
          <td>待评价</td>
          <td>2017-01-07.18.12.45</td>
          <td>2017-01-07.20.10.00</td>
        </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>

<script src="demo/html/clone.js" type="text/javascript"></script>