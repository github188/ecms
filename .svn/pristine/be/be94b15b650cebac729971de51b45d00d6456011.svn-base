.upload-logo-error {
    color: red;
    font-size: 14px;
    display: none;
}

.preview-logo {
    width: 120px;
    height: 120px;
    border-radius: 100%;
    border: 1px solid #ccc;
    margin-bottom: 15px;
}

.upload-logo {
    padding: 8px 10px;
    position: relative;
    color: #888;
    background: #fafafa;
    border: 1px solid #ddd;
    border-radius: 4px;
    overflow: hidden;
    display: block;
    width: 100px;
    margin: 0 auto;
}

.upload-logo input {
    position: absolute;
    right: 0;
    top: 0;
    opacity: 0;
    filter: alpha(opacity=0);
    width: 100%;
    height: 100%;
}

.upload-logo:hover {
    color: #444;
    background: #eee;
    border-color: #ccc;
    text-decoration: none
}

#basicInfoPDTab label {
    width: 70px;
    text-align: right;
}

#personnelInfoPDTab label {
    width: 84px;
    text-align: right;
}

#basicInfoPDTab input, #personnelInfoPDTab input {
    border: 0;
    padding-left: 0;
    padding-right: 0;
}

#basicInfoPDTab input {
    min-width: 180px;
}

#personnelInfoPDTab input {
    min-width: 222px;
}

.email-block label {
    display: block;
}

.email-block input {
    display: inline-block;
    width: 195px;
}

.email-block .email-suffix {
    font-size: 14px;
}

.add-depblock, .edit-depblock {
    margin: 0;
}

.add-depblock label:not(.error), .edit-depblock label:not(.error) {
    width: 85px;
    text-align: right;
}

.add-depblock input, .edit-depblock input {
    min-width: 280px;
}

#currentDepMsg {
    display: none;
}
