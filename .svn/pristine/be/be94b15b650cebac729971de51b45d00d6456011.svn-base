.z-index20 {
    z-index: 20;
}

#btnEditDepartment {
    width: 100%;
    margin-bottom: 20px;
}

.nav.nav-tabs {
    border-bottom: 1px solid #dddddd;
}

.nav.nav-tabs > li {
    float: left;
    margin-bottom: -1px;
}

.nav.nav-tabs > li > a {
    background-color: transparent;
    padding-left: 20px;
    padding-right: 20px;
    border: 1px solid transparent;
}

.nav.nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
    color: #555;
    cursor: default;
    background-color: #fff;
    border: 1px solid #ddd;
    border-bottom-color: transparent;
}

.nav.nav-tabs > li:not(.active) > a:hover {
    border: 1px solid transparent;
    background-color: #ccc;
}

.nav.nav-tabs + .tab-content {
    padding: 20px 0;
    margin-bottom: 10px;
}

.tab-content > .tab-pane {
    overflow: hidden;
}

.sex-radio {
    border: 0;
    padding: 6px 1px;
}

