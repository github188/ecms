@charset "UTF-8";
.tree-contain {
    padding-left: 15px;
    width: 100%;
    height: 560px;
    overflow-y: scroll;
}

#DepartForSuperiorAddPanel, #DepartForSuperiorEditPanel, #DepLeaderForAddDepPanel, #DepLeaderForEditDepPanel {
    width: 360px;
    background-color: #fff;
    border: 1px solid #666;
    margin-left: 19.5%;
    display: none;
}

#DepartForAddPerPanel, #ImmediateSuperiorPanel {
    width: 360px;
    background-color: #fff;
    display: none;
    position: fixed;
    z-index: 500;
    -webkit-box-shadow: 0 0 14px #999;
    -moz-box-shadow: 0 0 14px #999;
    box-shadow: 0 0 14px #999;
}

#DepartForAddPerPanel {
    top: 47%;
    left: 6%;
}

#ImmediateSuperiorPanel {
    top: 48%;
    left: 31%;
}

.close-tree-sel {
    float: right;
    display: inline-block;
    width: 13px;
    height: 13px;
    margin: 3px 5px;
    -webkit-transform: scale(2);
    transform: scale(2);
    color: #666;
    cursor: pointer;
}

.tree-contain-addper-sel, .tree-contain-ImmediateSuperior-sel {
    width: 100%;
    height: 280px;
    overflow-y: scroll;
    margin-top: 35px;
}

.tree-contain-adddep-sel, .tree-contain-depleader-sel {
    width: 100%;
    height: 280px;
    overflow-y: scroll;
    margin-top: 35px;
}

.ztree * {
    font-size: 14px;
}

.ztree li {
    line-height: 22px;
}

.ztree li span {
    line-height: 22px;
}

.ztree li a.curSelectedNode {
    height: 22px;
}

.ztree li span.button.switch {
    width: 18px;
    height: 22px;
}

.ztree li span.button.ico_open {
    vertical-align: middle;
}

.ztree li span.button.ico_docu {
    vertical-align: text-bottom;
}

.ztree li a.tmpTargetNode_inner {
    height: 22px;
}

