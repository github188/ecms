/*公用样式*/
.iconfont {
  color: #888;
  font-size: 18px;
  height: 20px;
  line-height: 20px;
  vertical-align: middle;
}

.side-new{
  cursor: pointer;
}

body, footer.main-footer {
  /*background-color: #e9eef1;*/
}

.side-new {
  padding: 8px 0px;
}

.side-new:hover, .side-new:active, .side-new:link, .side-new:visited {
  text-decoration: none;
}

.side-new .iconfont {
  color: #90929c;
}

.sidebar-menu .logo-env {
  border-bottom: none;
}

.sidebar-menu .main-menu {
  padding: 0;
}

.sidebar-menu .main-menu > li {
  font-size: 14px;
  border-left: 4px solid transparent;
}

.sidebar-menu .main-menu > li a {
  padding-left: 40px;
  padding-right: 20px;
}

.sidebar-menu.collapsed .main-menu > li a {
  padding: 13px 5px;
}

.sidebar-menu .main-menu > li li {
  font-size: 12px;
  padding-left: 30px;
}

.sidebar-menu .main-menu > li:hover > a, .sidebar-menu .main-menu > li.active > a, .sidebar-menu .main-menu > li.active {
  background: #f2f2f2;
  border-left-color: #01cd78;
}

.sidebar-menu .main-menu li li:hover, .sidebar-menu .main-menu > li li.active {
  background-color: #d2d2d2;
}

.sidebar-menu .main-menu li.active > a .iconfont, .sidebar-menu .main-menu li.active > a, .sidebar-menu .main-menu li.has-sub.active > a:before {
  color: #01cd78;
}

nav.navbar {
  background-color: #01cd78;
}

.navbar.horizontal-menu .navbar-inner .navbar-nav > li.active, .navbar.horizontal-menu .navbar-inner .navbar-nav > li:hover {
  background-color: #01cd78;
  border-radius: 0;
}

.sidebar-menu .main-menu a {
  color: #555;
}

.sidebar-menu .main-menu a:hover {
  color: #333;
}

.sidebar-menu .logo-env {
  border-top: 1px solid #eee;
}

.sidebar-menu .logo-env, .sidebar-menu, .sidebar-menu.fixed .sidebar-menu-inner {
  background-color: #fff;
  border-bottom: 1px solid #eee;
}

.sidebar-menu .main-menu a {
  border-bottom: none;
}

.sidebar-menu.collapsed .main-menu > li.hover {
  width: 303px;
  text-align: left;
  text-indent: 10px;
  box-shadow: 1px 1px 5px #ccc;
}

.sidebar-menu.collapsed .main-menu > li.hover span {
  display: inline-block;
}

.sidebar-menu.collapsed .main-menu > li.active, .sidebar-menu.collapsed .main-menu > li li.active {
  background-color: #ebf7ff;
}

.sidebar-menu.collapsed .main-menu > li > ul {
  background: #fff;
  box-shadow: 1px 1px 1px #eee;
}

.dataTable tbody tr:nth-child(2n) {
  background-color: #eff5f7;
}

/****************  【【 黑色（默认）   ****************/
.skin-default nav.navbar {
  background: #333;
}

.skin-default nav.navbar .alogo {
  border-right: 1px solid #666;
}

.skin-default .laydate_body .laydate_box a {
  color: #aaa !important;
}

.skin-default .laydate_body .laydate_top {
  border-top: 1px solid #333 !important;
  background-color: #333 !important;
}

.skin-default .laydate_body .laydate_ym {
  border: 1px solid #333 !important;
  background-color: #333 !important;
}

.skin-default .laydate_body .laydate_chprev, .skin-default .laydate_body .laydate_choose {
  border: none !important;
}

.skin-default .laydate_body .laydate_box a, .skin-default .laydate_body .laydate_box a:hover {
  color: #34424F;
  background-color: #333 !important;
}

.skin-default .laydate_body .laydate_click {
  background-color: #333 !important;
}

.skin-default .laydate_body .laydate_time .laydate_hmsno span:hover, .skin-default .laydate_body .laydate_table td:hover {
  background-color: #333 !important;
}

.skin-default .laydate_body .laydate_btn a:hover {
  color: #fff !important;
  background-color: #333 !important;
}

.skin-default .laydate_body .laydate_bottom .laydate_btn {
  border-right: 1px solid #333 !important;
}

.skin-default .laydate_body .laydate_bottom .laydate_btn a {
  border: 1px solid #eee !important;
}

.skin-default .laydate_body .laydate_table td.laydate_click {
  color: #fff;
}

.skin-default .laydate_body .laydate_ym .laydate_yms {
  border: 1px solid #333 !important;
  background-color: #fff !important;
  color: #333 !important;
}

.skin-default .btn-search, .btn-success, .btn-info {
  background-color: #333 !important;
}

.skin-default .btn.btn-turquoise {
  /*background-color: #f8a900 !important;*/
}

.skin-default .oprate:hover {
  color: #333 !important;
}

.skin-default input.form-control.form-focus-info:focus {
  border-color: rgba(51, 51, 51, 0.6) !important;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(51, 51, 51, 0.6) !important;
  -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(51, 51, 51, 0.6) !important;
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(51, 51, 51, 0.6) !important;
}

.skin-default .mapSearch .input-group-btn .btn-info {
  background: url("../img/map/search-icon.png") no-repeat center center #333 !important;
}

.skin-default .mapSearch .searchNav .area {
  background-color: #333 !important;
}

.skin-default .mapSearch .searchNav .type li.active {
  color: #333 !important;
}

.skin-default .mapSearch .navtab .row p.active {
  color: #fff !important;
  background-color: rgba(51, 51, 51, .8) !important;
}

.skin-default .parkinfoDiv {
  background: #3e3e3e !important;
}

.skin-default .search-date > a:hover, .skin-default .search-date > a.active, .skin-default .recharge-search > a:hover, .skin-default .recharge-search > a.active {
  border: 1px solid #333 !important;
  background: #333;
  color: #fff !important;
}

.skin-default .dropdown-menu.daterangepicker .calendar-date table > tbody > tr > td.active {
  background-color: #5D5D5D !important;
}

.skin-default .dropdown-menu.daterangepicker .calendar-date table > tbody > tr > td.in-range {
  background-color: rgba(51, 51, 51, 0.1);
}

.skin-default #complainWorkInfo {
  border-top: 15px solid #333 !important;
}

