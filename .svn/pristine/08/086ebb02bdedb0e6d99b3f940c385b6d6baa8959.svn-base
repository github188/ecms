@charset "utf-8";
.pages {
  margin: 16px auto;
  overflow: hidden;
}
.pages .page-inline {
  display: inline-block;
  *display: inline;
  zoom: 1;
}
.pages #pagination {
  float: left;
}
.pages #pagination .pagination {
  margin: 0 auto;
  text-align: right;
}
.pages #pagination .pagination a,
.pages #pagination .pagination span {
  float: left;
  margin-right: 10px;
  display: inline;
  padding: 3px 9px;
  background: #fff;
  border: 1px solid #fff;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  color: #c9c9c9;
  font-size: 14px;
  cursor: pointer;
}
.pages #pagination .pagination .current {
  background: #fff;
  border-color: #ff6600;
  color: #ff6600;
}
.pages #pagination .pagination a:hover {/* .pages #pagination .pagination span:hover, */
  text-decoration: none;
  color: #ff6600;
}
.pages #pagination .pagination .prev,
.pages #pagination .pagination .next {
  float: left;
  padding: 3px 9px;
  border: 1px solid #fff;
  background: #fff;
  color: #c9c9c9;
  cursor: pointer;
}
.pages #pagination .pagination .prev:hover,
.pages #pagination .pagination .next:hover {
  color: #ff6600;
}
.pages #pagination .pagination .prev i,
.pages #pagination .pagination .next i {
  display: inline-block;
  width: 4px;
  height: 11px;
  margin-right: 5px;
}
.pages #pagination .pagination .prev i {
  background-position: -144px -1px;
  *background-position: -144px -4px;
}
.pages #pagination .pagination .next i {
  background-position: -156px -1px;
  *background-position: -156px -4px;
}
.pages #pagination .pagination .pagination-break {
  padding: 6px 15px;
  border: none;
  background: none;
  cursor: default;
}
.pages .searchPage {
  float: left;
  padding: 8px 0;
}
.pages .searchPage .page-sum {
  padding: 11px 13px;
  color: #999999;
  font-size: 14px;
}
.pages .searchPage .page-go {
  padding: 8px 0;
  color: #999999;
  font-size: 14px;
  padding: 10px 0\9;
  *padding: 6px 0;
}
.pages .searchPage .page-go input {
  width: 30px;
  margin: 0 5px;
  padding: 0 5px;
  border: 1px solid #e4e4e4;
}
.pages .searchPage .page-btn {
  margin: 9px 0 5px 5px;
  padding: 2px 5px;
  background: #18a0b0;
  border-radius: 2px;
  color: #ffffff;
  font-size: 14px;
  text-decoration: none;
}
