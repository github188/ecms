<!-- 修改考勤记录 -->
<div class="modal fade" id="editAttendanceModal" data-backdrop="static" data-clear="true">
    <div class="modal-dialog" style="width: 640px">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">修改考勤记录</h4>
            </div>
            <form class="form-validate" id="editAttendanceForm" role="form">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12 form-group">
                            <label class="col-md-2 control-label"><span class="required">*</span>日期</label>
                            <div class="col-md-10">
                                <input class="form-control" id="editlayDate" name="layDate" type="text" readonly="readonly">
                            </div>
                        </div>

                        <div class="col-md-12 form-group">
                            <label class="col-md-2 control-label"><span class="required">*</span>状态</label>
                            <div class="col-md-10">
                                <select class="form-control" name="status">
                                    <option value="0">休（周末及法定节假日）</option>
                                    <option value="1">正常</option>
                                    <option value="2">休年假</option>
                                    <option value="">差旅</option>
                                    <option value="">年休假</option>
                                    <option value="">调休假</option>
                                    <option value="">病假</option>
                                    <option value="">事假</option>
                                    <option value="">有薪假</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-12 form-group">
                            <label class="col-md-2 control-label"><span class="required">*</span>时长</label>
                            <div class="col-md-10">
                                <select class="form-control" name="duration">
                                    <option value="0">1天</option>
                                    <option value="1">0.5天</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-md-12 form-group">
                            <label class="col-md-2 control-label"><span class="required">*</span>修改备注</label>
                            <div class="col-md-10">
                                <textarea class="form-control" name="remark" style="height: 100px"></textarea>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <div class="modal-msg"></div>
                    <button class="btn btn-success btn-examine" type="submit">确定</button>
                    <button type="reset" class="btn btn-white" data-dismiss="modal">关闭</button>
                </div>
            </form>
        </div>
    </div>
</div>
