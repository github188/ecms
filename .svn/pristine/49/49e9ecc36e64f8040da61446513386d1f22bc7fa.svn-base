<!-- 分配任务 -->
<div class="modal fade" id="assignTaskModal" data-backdrop="static">
    <div class="modal-dialog" style="width: 780px">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">分配任务</h4>
            </div>
            <form method="post" class="validate" id="assignTaskForm">
                <div class="modal-body">
                    <div class="row form-inline">
                        <div class="form-group col-sm-6">
                            <label class="control-label"><span class="required">*</span>任务启动日期：</label>
                            <input type="text" class="form-control laydate-icon" name="taskStartTime" id="taskStartTime" readonly placeholder="请选择日期"/>
                        </div>
                        <div class="form-group col-sm-6">
                            <label class="control-label"><span class="required">*</span>任务截止日期：</label>
                            <input type="text" class="form-control laydate-icon" name="taskEndTime" id="taskEndTime" readonly placeholder="请选择日期"/>
                        </div>
                        <div class="form-group col-sm-12">
                            <label class="control-label"><span class="required">*</span>任务相关成员：</label>
                            <input type="text" class="form-control input-key" name="taskOfMebs" id="taskOfMebs" readonly placeholder="请选择任务相关人员"/>
