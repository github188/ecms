<!-- 新增需求弹窗 -->
<div class="modal fade" id="demandEditPanel" data-backdrop="static" data-clear="true">
    <div class="modal-dialog" style="width: 640px">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">新建需求</h4>
            </div>
            <form class="formvalidate form-validate" id="demandEditForm" method="post" enctype="multipart/form-data" role="form">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12 form-group">
                            <label class="col-md-2 control-label"><span class="required" aria-required="true">*</span>项目名称</label>
                            <div class="col-md-10">
                                <div class="search-boxs errorPlacement">
                                    <input class="form-control" name="proInfo" type="text" autocomplete="off" readonly="readonly">
                                    <div class="renew-search-content pro-search">
                                        <div class="renew-search-border"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 form-group">
                            <label class="col-md-2 control-label"><span class="required" aria-required="true">*</span>需求接收人</label>
                            <div class="col-md-10">
                                <div class="search-boxs errorPlacement">
                                    <input class="form-control" id="receivePerson" name="receivePerson" type="text" autocomplete="off" readonly="readonly">
                                    <!--<div class="renew-search-content user-search">-->
                                        <!--<div class="renew-search-border"></div>-->
                                    <!--</div>-->
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 form-group errorPlacement">
                            <label class="col-md-2 control-label"><span class="required" aria-required="true">*</span>是否紧急</label>
                            <div class="col-md-10 radio">
                            	<label class="radio-inline">
                                    <input name="reqEmergency" type="radio" value="2" checked="checked">
                                    <span class="lbl"></span><span class="radio-text">否</span>
                                </label>
                                <label class="radio-inline">
                                    <input name="reqEmergency" type="radio" value="1" >
                                    <span class="lbl"></span><span class="radio-text">是</span>
                                </label>
                            </div>
                        </div>
                        <div class="col-md-12 form-group chuliren">
                            <label class="col-md-2 control-label"><span class="required" aria-required="true">*</span>需求标题</label>
                            <div class="col-md-10">
                                <input class="form-control" name="reqtitle" type="text">
                            </div>
                        </div>
                        <div class="col-md-12 form-group">
                            <label class="col-md-2 control-label"><span class="required" aria-required="true">*</span>需求详情</label>
                            <div class="col-md-10">
                                <div class="wangeditor-container" id="reqDetail"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="modal-msg"></div>
                    <button class="btn btn-success btn-examine" type="submit" id="btnToAddDMD">确定</button>
                    <button type="reset" class="btn btn-white" data-dismiss="modal">关闭</button>
                </div>
            </form>
        </div>
    </div>
</div>
