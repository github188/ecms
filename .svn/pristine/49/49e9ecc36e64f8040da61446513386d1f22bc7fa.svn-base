                                    <label class="control-label">是否需要二审：</label>
                                    <select class="form-control adt-sel" id="adtIsNeedTwoadt" name="adtIsNeedTwoadt">
                                        <option value="">请选择</option>
                                        <option value="1">是</option>
                                        <option value="2">否</option>
                                    </select>
                                    <span class="error-msg"></span>
                                </div>
                                <div class="form-group col-sm-12 display-none" id="twoAdtContain">
                                    <input type="text" class="form-control" id="twoAdtMeb" name="twoAdtMeb" readonly placeholder="请选择二审人员"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer display-none">
                    <div class="modal-msg"></div>
                    <button type="submit" class="btn btn-success" id="btnAdtOne">提交</button>
                    <button type="reset" class="btn btn-white" data-dismiss="modal">取消</button>
                </div>
            </form>
        </div>
    </div>
</div>

