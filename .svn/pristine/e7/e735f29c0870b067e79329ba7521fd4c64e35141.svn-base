package com.ecaray.ecms.entity.news;

import java.util.List;

import com.ecaray.ecms.entity.process.ProcessBase;

public class PortalProcess extends ProcessBase{

	private Integer processType = 6;
    private String name;

    private String headPic;

    private String content;
    
    private String userName;

    private String typeName;
    
    private Integer type;
    
    private String newsTitle;
    
    private List<PortalFiles> files;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getHeadPic() {
		return headPic;
	}

	public void setHeadPic(String headPic) {
		this.headPic = headPic;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}

	public List<PortalFiles> getFiles() {
		return files;
	}

	public void setFiles(List<PortalFiles> files) {
		this.files = files;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public Integer getProcessType() {
		return processType;
	}

	public void setProcessType(Integer processType) {
		this.processType = processType;
	}

	public String getNewsTitle() {
		return newsTitle;
	}

	public void setNewsTitle(String newsTitle) {
		this.newsTitle = newsTitle;
	}
}