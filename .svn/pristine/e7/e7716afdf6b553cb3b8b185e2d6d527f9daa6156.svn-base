#DepartForSuperiorEditPanel {
    width: 300px;
    background-color: #fff;
    border: 1px solid #666;
    display: none;
    position: absolute;
    left: 106px;
    z-index: 99;
}

#DepartForAddPerPanel, #ImmediateSuperiorPanel {
    width: 360px;
    background-color: #fff;
    display: none;
    position: fixed;
    z-index: 500;
    -webkit-box-shadow: 0 0 14px #999;
    -moz-box-shadow: 0 0 14px #999;
    box-shadow: 0 0 14px #999;
}
.clear-depart{
    position: absolute;
    right: 46px;
    top: 6px;
    font-size: 16px;
    cursor: pointer;
    z-index: 10;
    display: none;
}
.close-tree-sel {
    float: right;
    display: inline-block;
    width: 13px;
    height: 13px;
    margin: 3px 5px;
    -webkit-transform: scale(2);
    transform: scale(2);
    color: #666;
    cursor: pointer;
}
.tree-contain-addper-sel, .tree-contain-ImmediateSuperior-sel {
    width: 100%;
    height: 280px;
    overflow-y: scroll;
    margin-top: 35px;
}
.tree-contain-adddep-sel, .tree-contain-depleader-sel {
    width: 100%;
    height: 280px;
    overflow-y: scroll;
    margin-top: 35px;
}

.ztree * {
    font-size: 14px;
}

.ztree li {
    line-height: 22px;
}

.ztree li span {
    line-height: 22px;
}

.ztree li a.curSelectedNode {
    height: 22px;
}

.ztree li span.button.switch {
    width: 18px;
    height: 22px;
}

.ztree li span.button.ico_open {
    vertical-align: middle;
}

.ztree li span.button.ico_docu {
    vertical-align: text-bottom;
}

.ztree li a.tmpTargetNode_inner {
    height: 22px;
}
#treeDepart{
    height: 550px;
    overflow: auto;
}
#editAttendanceForm label.control-label{
    text-align: right;
}
.input-tag{
    position: absolute;
    top: 2px;
    left: 22px;
}
.tag-list{
    display: inline-block;
    color: #01cd78 ;
    border-radius: 3px;
    font-size: 12px;
    padding: 4px 12px;
    padding-right: 20px;
    position: relative;
    margin-top: 5px;
    margin-right: 15px;
    border: 1px solid #01cd78 ;
}
.tag-list:hover,.tag-list:active,.tag-list:visited, .tag-list:focus{
    color: #01cd78 ;
}
.remove-tag{
    position: absolute;
    right: 6px;
    font-size: 14px;
    top: 2px;
}
.list-top {
    border-bottom: 1px solid #e6e5e5;
}
.list-top {
    border-bottom: 1px solid #e6e5e5;
}
.list-top .list-title {
    margin-top: 27px;
    margin-bottom: 26px;
}
.list-top .list-title:before {
    background: #01cd78;
}
.form-group .control-label{
    font-size: 14px;
}
.clear-person{
    position: absolute;
    right: 32px;
    top: -2px;
    font-size: 18px;
    color: #999;
    display: none;
    padding: 10px;
}
.row-list:hover .clear-person{
    display: block;
}
.approvePerson{
    height: 40px;
    cursor: pointer
}
.z-hide{
    display: none;
}
