@charset "UTF-8";
ul.ascription li {
    cursor: pointer;
    list-style: none;
    float: left;
    margin: 0px 5px;
    padding: 2px 5px;
    border-bottom: 2px solid #fff;
}

ul.ascription li:first-child {
    margin-left: 0px;
}

ul.ascription li.active, ul.ascription li:hover {
    color: #01cd78;
    border-bottom: 2px solid #01cd78;
}

.z-index20 {
    z-index: 20;
}

.tree-contain {
    padding-left: 15px;
    width: 100%;
    height: 350px;
    overflow-y: scroll;
}

.ztree * {
    font-size: 14px;
}

.ztree li {
    line-height: 22px;
}

.ztree li span {
    line-height: 22px;
}

.ztree li a.curSelectedNode {
    height: 22px;
}

.ztree li span.button.switch {
    width: 18px;
    height: 22px;
}

.ztree li span.button.ico_open {
    vertical-align: middle;
}

.ztree li span.button.ico_docu {
    vertical-align: text-bottom;
}

.addclient-contain input, .addclient-contain select {
    min-width: 235px;
}

.addclient-contain .desc-textarea, .addclient-contain input#dpromebs2 {
    min-width: 90%;
}

.addclient-contain .pro-desc-label {
    display: inline-block;
    vertical-align: top;
}

.addclient-contain input.c-address {
    min-width: 311px;
}

.contact-contain {
    border-top: 2px solid #ccc;
    margin-top: 30px;
    padding-top: 15px;
}

.contact-contain .form-control {
    width: 100%;
    padding: 6px 6px;
}

.icon-add, .icon-del {
    display: inline-block;
    width: 24px;
    margin-right: 18px;
    margin-top: 5px;
}






