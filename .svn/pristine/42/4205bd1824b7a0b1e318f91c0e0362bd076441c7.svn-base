public class NewsComWithChildren extends NewsCom {
    private List<NewsComWithChildren>  children;
    private User user;
    private Integer likes;
    private Integer isLikes;
    
    public List<NewsComWithChildren> getChildren() {
        return children;
    }

    public void setChildren(List<NewsComWithChildren> children) {
        this.children = children;
    }

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public Integer getLikes() {
		return likes;
	}

	public void setLikes(Integer likes) {
		this.likes = likes;
	}

	public Integer getIsLikes() {
		return isLikes;
	}

	public void setIsLikes(Integer isLikes) {
		this.isLikes = isLikes;
	}
}
